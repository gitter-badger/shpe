<% provide(:title, 'Resources') %>

<div class="container">
  <div class="row">
    <div class="span9 center">
      <h1>Resources</h1>
      <ul class="nav nav-tabs">
        <li><a href="#fallhsv" data-toggle="tab">Fall HSV</a></li>
        <li><a href="#career" data-toggle="tab">Careers</a></li>
        <li><a href="#scholarships" data-toggle="tab">Scholarships</a></li>



