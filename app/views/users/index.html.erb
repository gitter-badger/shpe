<% provide(:title, 'All users') %>

<div class="container">
  <div class="row">
    <div class="span6 offset3">
      <h2>All users</h2>

      <% for user in @users %>
          <li>
            <%= link_to user.name, view_user_path(user) %>

          </li>


      <% end %>
    </div>
  </div>
</div>