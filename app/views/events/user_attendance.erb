

<div class="container">
  <div class="row">
    <div class="span8 offset3">
      <h2><%= @event.name %>   </h2>

      <table>
        <tr>
          <th>Name</th>
          <th>Email</th>
          <th>UIN</th>

        </tr>

        <% @users.each do |user| %>
            <tr>
              <td><%= user.name %></td>
              <td><%= user.email %></td>
              <td><%= user.uin %></td>

            </tr>
        <% end %>
      </table>

      <%= link_to 'All Events', events_path, class: 'btn' %>
    </div>
  </div>
</div>


